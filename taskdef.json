{
    "executionRoleArn": "arn:aws:iam::980070691560:role/ECSRole",
    "containerDefinitions": [
        {
            "name": "RxTracker-Frontend",
            "image": "980070691560.dkr.ecr.us-east-1.amazonaws.com/rx_tracker_frontend_nginx:latest",
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 80,
                    "protocol": "tcp",
                    "containerPort": 80
                }
            ]
        }
    ],
    "requiresCompatibilities": [
        "EC2"
    ],
    "networkMode": "awsvpc",
    "cpu": "256",
    "memory": "512",
    "family": "ecs-demo"
}